{
  "name": "Question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "questionText": {
      "type": "string",
      "required": true
    },
    "answerText": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true
    },
    "upVote": {
      "type": "number",
      "default": 0
    },
    "downVote": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": ""
    },
    "answers": {
      "type": "hasMany",
      "model": "Answer",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
