{
  "name": "Question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "questionText": {
      "type": "string",
      "required": true
    },
    "answerText": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true
    },
    "upVote": {
      "type": "number",
      "default": 0
    },
    "downVote": {
      "type": "number",
      "default": 0
    },
    "userId": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": ""
    },
    "answers": {
      "type": "hasMany",
      "model": "Answer",
      "foreignKey": ""
    },
    "users": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
